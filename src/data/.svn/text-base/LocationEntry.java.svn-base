package data;



import java.io.Serializable;
/**
 * LocationEntry stores the basic information of a location entry such as the location name and its coordinates (stored in Doubles).
 * @author  Mihai
 */
public class LocationEntry implements Serializable{

	private static final long serialVersionUID = 1539186924425908572L;
	private String location_name;
	private Double latitude;
	private Double longitude;
	private Double elevation;
	//private Double latitude;
	//private Double longitude;
	//private Double elevation;
	
	/**
	 * Constructor; initiates the values of this location entry (the location name, latitude, longitude and
	 * elevation
	 */
	public LocationEntry(String locationName, Double latitude_arg, Double longitude_arg, Double elevation_arg) {
		this.location_name = locationName;
		this.latitude = latitude_arg;
		this.longitude = longitude_arg;
		this.elevation = elevation_arg;
	}
	
	/**
	 * returns the latitude of a LocationEntry object
	 */
	public Double getLatitude() {
		return latitude;
	}
	
	/**
	 * set the latitude of a LocationEntry object
	 */
	public void setLatitude(Double latitude_arg) {
		latitude = latitude_arg;
	}
	
	/**
	 * returns the longitude of a LocationEntry object
	 */
	public Double getLongitude() {
		return longitude;
	}
	
	/**
	 * set the longitude of a LocationEntry object
	 */
	public void setLongitude(Double longitude_arg) {
		longitude = longitude_arg;
	}
	
	/**
	 * returns the elevation of a LocationEntry object
	 */
	public Double getElevation() {
		return elevation;
	}
	
	/**
	 * set the elevation of a LocationEntry object
	 */
	public void setElevation(Double elevation_arg) {
		elevation = elevation_arg;
	}
	
	/**
	 * returns the location name of a LocationEntry object
	 */
	public String getLocationName() {
		return location_name;
	}
	
	/**
	 * set the location name of a LocationEntry object
	 */
	public void setLocationName(String locationName) {
		this.location_name = locationName;
	}
	
}
